{
  "name": "seedling-hq",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "pnpm --filter @seedling/api run dev & pnpm --filter @seedling/web run dev",
    "build": "pnpm -r run build",
    "gen": "pnpm --filter @seedling/api run gen",
    "lint": "pnpm -r run lint",
    "typecheck": "pnpm -r run typecheck",
    "test": "pnpm -r run test",
    "test:integration": "pnpm --filter @seedling/api run test:integration",
    "test:e2e": "pnpm --filter @seedling/e2e run test",
    "services:up": "docker compose up -d",
    "services:down": "docker compose down",
    "services:logs": "docker compose logs -f",
    "db:push": "pnpm --filter @seedling/api run db:push",
    "db:seed": "pnpm --filter @seedling/api run db:seed",
    "db:reset": "pnpm --filter @seedling/api run db:reset"
  }
}
